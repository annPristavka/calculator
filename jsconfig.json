{
    "compilerOptions": {
      "target": "es6",
      "baseUrl": ".",
      "paths": {
        "@/*": ["./src/*"]
      },
      "module": "commonjs"
    },
    "exclude": ["node_modules"],
    "paths": {
      "@components/*": ["components/*"],
      "@styled/*": ["components/styled/*"],
      "@pages/*": ["components/pages/*"],
      "@assets/*": ["assets/*"],
      "@services/*": ["services/*"]
    }
  }
  